{
    "manifest_version": 3,
    "name": "Password Manager Extension",
    "version": "1.0",
    "permissions": ["activeTab", "storage", "http://localhost:3000/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
    
  }
  